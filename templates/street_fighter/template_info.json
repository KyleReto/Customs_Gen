{
    "parser_version":1.0,
    "creator":"Tirankin",
    "data":{
        "character":{
            "character_background":{
                "type":"image",
                "path":"templates/street_fighter/images/character_background.png"
            },
            "card_art":"{config,images,character_image}",
            "character_frame":{
                "type":"image",
                "path":"templates/street_fighter/images/character_frame.png"
            },
            "character_critical":{
                "type":"image",
                "path":"templates/street_fighter/images/character_critical.png",
                "show_if":"{card,secondary_type}",
                "show_equals":"Critical"
            },
            "exceed_flip":{
                "type":"image",
                "path":"templates/street_fighter/images/exceed_flip.png",
                "show_if":"{card,cost}",
                "show_not_equal":""
            },
            "exceed_cost":{
                "type":"text",
                "content":"{card,cost}",
                "show_if":"{card,cost}",
                "show_not_equal":"",
                "fonts":["templates/street_fighter/fonts/WCManoNegraBoldBta.otf"],
                "bounding_box":[660,830,710,910],
                "default_font_size":64,
                "default_line_height":64,
                "outline":[3,"#FFFFFF"]
            },
            "character_effect":{
                "type":"text",
                "content":"{card,text_box}",
                "text_to_image":{
                    " --":{
                        "path":"templates/street_fighter/images/critical_icon.png",
                        "dest_offsets":[-5,0]
                    }
                },
                "fonts":["templates/street_fighter/fonts/FiraSansExtraCondensed-Regular.ttf",
                "templates/street_fighter/fonts/FiraSansExtraCondensed-Bold.ttf",
                "templates/street_fighter/fonts/FiraSansExtraCondensed-Italic.ttf",
                "templates/street_fighter/fonts/FiraSansExtraCondensed-BoldItalic.ttf"],
                "bounding_box":[100,750,650,970],
                "default_font_size":33,
                "default_line_height":40
            },
            "character_name":{
                "type":"text",
                "content":"{card,card_name}",
                "fonts":["templates/street_fighter/fonts/WCManoNegraBoldBta.otf"],
                "bounding_box":[20,660,400,751],
                "default_font_size":88,
                "default_line_height":88,
                "align": "left",
                "outline":[5,"#FFFFFF"]
            },
            "watermark":{
                "type":"text",
                "content":"FAN CARD NOT OFFICIAL. Exceed Â© Level 99 Games. Template by {template,creator}. Card by {config,creator}'. All assets copyright their respective owners.",
                "fonts":["templates/street_fighter/fonts/FiraSansExtraCondensed-Bold.ttf"],
                "bounding_box":[0,1000,750,1024],
                "default_font_size":14,
                "default_line_height":14,
                "text_color":"#FFFFFF",
                "outline":[2,"#000000"]
            }
        }
    }
}